<h1>Songs</h1>
<div><%= link_to 'Create Song', new_song_path %></div>

<table>
    <tr>
        <td>Title</td> 
        <td>Released</td>
        <td>Release Year</td> 
        <td>Artist Name</td> 
        <td>Genre</td>
    </tr>
        <tbody>
          <% @songs.each do |song| %>
          <td> <%= link_to song.title, song_path(song) %> </td>
          <td><%= song.released %></td>
          <td> <%= song.release_year %></td>
          <td> <%= song.artist_name %></td>
          <td> <%= song.genre %></td>
        </tbody>
<% end %>

</table>